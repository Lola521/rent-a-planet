<h3>Modifier <%= @planet.name %></h3>

<%= simple_form_for(@planet) do |f| %>
  <%= f.input :name %>
  <%= f.input :planet_type, collection: Planet::TYPEPLANET %>
  <%= f.input :safety, collection: Planet::SAFETYPLANET %>
  <%= f.input :description %>
  <%= f.input :prix %>
  <%= f.input :localisation, collection: Planet::LOCALISATIONPLANET %>
  <%= f.input :photo, as: :file %>
  <%= f.button :submit %>
<% end %>
